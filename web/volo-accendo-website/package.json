{
  "name": "volo-accendo-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .next node_modules out ./tmpBuildLambdas",
    "clean:lambdas:dev": "rm -rf ./.tmpBuildLambdas ./out/dev/lambdas",
    "clean:lambdas:prod": "rm -rf ./tmpBuildLambdas ./out/prod/lambdas",
    "clean:lambdas": "pnpm clean:lambdas:prod",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "cp .env.prod .env && next build && pnpm build:lambdas",
    "build:dev": "cp .env.dev .env && next build && pnpm build:lambdas:dev && pnpm copy:static:dev && pnpm copy:public:dev",
    "build:prod": "pnpm build",
    "compile:lambdas": "tsc -p tsconfig.lambdas.json",
    "copy:static:dev": "mkdir -p ./out/dev/next/standalone/out/dev/next/server/app/_next/ && cp -r ./out/dev/next/static ./out/dev/next/standalone/out/dev/next/server/app/_next/",
    "build:lambdas:dev": "pnpm clean:lambdas:dev && pnpm compile:lambdas && pnpm copy:lambdas:dev",
    "build:lambdas:prod": "pnpm compile:lambdas && mkdir -p ./out/prod/lambdas && mv ./.tmpBuildLambdas/* ./out/prod/lambdas && rm -rf ./.tmpBuildLambdas",
    "build:lambdas": "pnpm clean:lambdas && pnpm build:lambdas:prod",
    "copy:lambdas:dev": "mkdir ./out/dev/lambdas && mv ./.tmpBuildLambdas/* ./out/dev/lambdas && rm -rf ./.tmpBuildLambdas && cp -r ./out/dev/next/standalone/* ./out/dev/lambdas",
    "copy:public:dev": "cp -r ./public/* ./out/dev/next/standalone/out/dev/next/server/app/",
    "zip:lambdas:dev": "cd out/dev/lambdas && zip -r ../nextjs-lambda-edge.zip .",
    "zip:lambdas:prod": "cd out/prod/lambdas && zip -r ../nextjs-lambda-edge.zip .",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "next": "15.1.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.146",
    "@types/node": "^20.14.13",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "daisyui": "^4.12.14",
    "dotenv-cli": "^7.4.2",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "postcss": "^8.4.40",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@9.10.0+sha512.73a29afa36a0d092ece5271de5177ecbf8318d454ecd701343131b8ebc0c1a91c487da46ab77c8e596d6acf1461e3594ced4becedf8921b074fbd8653ed7051c"
}